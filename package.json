{
  "name": "gatsby-starter-minimal",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "salexpro <dev@salex.pro>",
  "contributors": [
    {
      "name": "Savelii Lukash",
      "email": "SaveliiLukash@gmail.com"
    }
  ],
  "dependencies": {
    "@gatsbyjs/reach-router": "1.3.7",
    "@gvrs/gatsby-transformer-blurhash": "1.0.4",
    "@percy/cli": "1.0.5",
    "@svgr/webpack": "6.2.1",
    "bootstrap": "5.1.3",
    "classnames": "2.3.1",
    "gatsby": "4.12.1",
    "gatsby-plugin-image": "2.12.1",
    "gatsby-plugin-minify-classnames": "4.1.2",
    "gatsby-plugin-offline": "5.12.1",
    "gatsby-plugin-react-helmet": "5.12.1",
    "gatsby-plugin-sass": "5.12.1",
    "gatsby-plugin-sharp": "4.12.1",
    "gatsby-plugin-svgr-svgo": "2.0.0",
    "gatsby-source-filesystem": "4.12.1",
    "gatsby-transformer-sharp": "4.12.1",
    "gsap": "3.10.3",
    "prop-types": "15.8.1",
    "react": "18.0.0",
    "react-bootstrap": "2.2.3",
    "react-dom": "18.0.0",
    "react-helmet": "6.1.0",
    "sass": "1.50.0",
    "svg-parser": "2.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.27",
    "husky": "7.0.4",
    "nano-staged": "0.6.0",
    "prettier": "2.6.2",
    "stylelint": "14.6.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-prettier": "2.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0 -o",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json,mdx,md,css,scss}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "copy-bs-settings": "node ./scripts/copy-bs-settings.js",
    "prepare": "husky install && yarn copy-bs-settings",
    "cz": "cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "nano-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint"
    ],
    "*.{css,scss}": [
      "stylelint --allow-empty-input"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
